{
  "id": "todos",
  "label": "ToDo Item Model",
  "isServer": true,
  "isClient": true,
  "type": "Default",
  "store": "loki",
  "prefetch": true,
  "debug": true,

	"defaults": {
    "label": "New Task"
  },

	"schema": {
    "label": {
      "validators": ["required"]
    },
    "complete": {
      "facet": true
    },
    "dueDate": {
      "facet": true
    }
  },
  "filter": false
}
